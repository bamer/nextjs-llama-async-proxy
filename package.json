{
  "name": "nextjs-llama-async-proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx server.js",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type:check": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "db:export": "node -e \"const fs = require('fs'); const path = require('path'); const backupDir = path.join(process.cwd(), 'data', 'backup'); if (!fs.existsSync(backupDir)) { fs.mkdirSync(backupDir, { recursive: true }); } const { exportDatabase } = require('./src/lib/database'); exportDatabase(path.join(backupDir, 'llama-dashboard-backup.db'));\"",
    "db:import": "node -e \"const path = require('path'); const backupPath = path.join(process.cwd(), 'data', 'backup', 'llama-dashboard-backup.db'); const { importDatabase } = require('./src/lib/database'); importDatabase(backupPath);\"",
    "db:vacuum": "node -e \"const { vacuumDatabase } = require('./src/lib/database'); vacuumDatabase();\"",
    "db:reset": "node -e \"const { initDatabase, deleteAllModels } = require('./src/lib/database'); const db = initDatabase(); deleteAllModels(); console.log('Database reset: all models deleted');\""
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@jest/globals": "^30.2.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/utils": "^7.3.6",
    "@mui/x-charts": "^8.23.0",
    "@mui/x-date-pickers": "^8.23.0",
    "@sentry/nextjs": "^10.32.1",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@tanstack/react-query": "^5.90.14",
    "@tanstack/react-query-devtools": "^5.91.2",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "node-fetch": "^3.3.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@emotion/jest": "^11.14.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "better-sqlite3": "^12.5.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "next-themes": "^0.4.6",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
